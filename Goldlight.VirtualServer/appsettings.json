{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LocalStack": {
    "UseLocalStack": true,
    "Session": {
      "AwsAccessKeyId": "goldlight",
      "AwsAccessKey": "goldlight",
      "AwsSessionToken": "my-AwsSessionToken",
      "RegionName": "eu-west-2"
    },
    "Config": {
      "LocalStackHost": "localhost",
      "UseSsl": false,
      "UseLegacyPorts": false,
      "EdgePort": 4566
    }
  },
  "Keycloak": {
    "realm": "SV",
    "auth-server-url": "http://localhost:8080/",
    "ssl-required": "none",
    "resource": "service_virtualization",
    "verify-token-audience": true,
    "credentials": {
      "secret": ""
    },
    "confidential-port": 0
  },
  "Postgres": {
    "host": "localhost",
    "port": "5432",
    "user": "sv_user",
    "password": "svpassword",
    "database":  "goldlight"
  }
}
